{
  "manifest_version": 3,
  "name": "Hacker News Gruvbox Dark Hard",
  "version": "1.0",
  "description": "Applies the Gruvbox Dark Hard theme to Hacker News",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://news.ycombinator.com/*"
      ],
      "css": [
        "styles.css"
      ]
    }
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}